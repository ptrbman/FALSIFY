void max_fact_0() {
  int a = 0;
  int b = 1;
  int ret = max(a,b);
  #FACT ret == 1
}

void max_fact_1() {
  int a = 0;
  int b = _;
  int ret = max(a,b);
  #FACT ret == 0
}

void max_fact_2() {
  int a = 0;
  int b = _;
  #ASSUME b < 0
  int ret = max(a,b);
  #FACT ret == 0
}

void max_fact_4() {
  int a = _;
  int b = _;
  #ASSUME a > 0
